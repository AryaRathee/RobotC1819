task main()
{
	startMotor(port2, 63); //start motor
	wait(5);        //wait 5 sec
	stopMotor(port2);       //stop motor

	startMotor(port3, -95); //start motor
	wait(5);        //wait 5 sec
	stopMotor(port3);       //stop motor 

	startMotor(port3, 127); //both motors
	startMotor(port2, 127);
	wait(5);        //wait 5 sec
	stopMotor(port2);
	stopMotor(port3);


}
